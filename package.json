{
  "name": "lint-test",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:CaptainJack-Git/lint-test.git",
  "author": "CaptainJack <captain_jack_mail@163.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1"
  },
  "scripts": {
    "fix": "eslint --fix *.js",
    "eslint": "eslint *.js",
    "quick": "pretty-quick --staged",
    "lint": "pretty-quick --staged && lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint *.js"
    ]
  }
}
